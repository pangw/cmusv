<h1>New deliverable</h1>

<% form_for (@deliverable), :html => {:multipart => true} do |f| %>
    <% #form_for :deliverable, :html => {:multipart => true } do |f| %>

  <%= f.error_messages %>

  <p>
    <%= f.label :team_id %><br />
    <%= f.select :team_id, @deliverable.uploader.teams.collect{|team|[team.name, team.id]} %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :comments %><br />
    <%= f.text_field :comments %>
  </p>
  <p>
    <%= f.label :task_number %><br />
    <%= f.text_field :task_number %>
  </p>
  <p>
    <%= f.label :course_number_id %><br />
    <%= f.text_field :course_number_id %>
  </p>
  <p>
    <%= f.label :is_individual %><br />
    <%= f.check_box :is_individual %>
  </p>
   <p>
     <%= f.file_field :attachment %>
    </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', deliverables_path %>